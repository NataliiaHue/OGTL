                </data>
                <siteModel id="SiteModel.s:transeurasian_stable" spec="SiteModel" gammaCategoryCount="1" mutationRate="@mutationRate.s:transeurasian_stable">
                    <parameter id="gammaShape.s:transeurasian_stable" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:transeurasian_stable" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
