        </tree>
        <parameter id="mutationRate.s:transeurasian" name="stateNode">1.0</parameter>
        <parameter id="bcov_alpha.s:transeurasian" lower="1.0E-4" name="stateNode" upper="1.0">0.5</parameter>
        <parameter id="bcov_s.s:transeurasian" lower="1.0E-4" name="stateNode" upper="Infinity">0.5</parameter>
        <parameter id="frequencies.s:transeurasian" dimension="2" lower="0.0" name="stateNode" upper="1.0">0.5 0.5</parameter>
        <parameter id="birthRate.t:tree" name="stateNode">1.0</parameter>
        <parameter id="ucldStdev.c:clock" lower="0.0" name="stateNode">0.1</parameter>
        <stateNode id="rateCategories.c:clock" spec="parameter.IntegerParameter" dimension="74">1</stateNode>
        <parameter id="ucldMean.c:clock" name="stateNode">1.0</parameter>
    </state>

    <init id="RandomTree.t:tree" spec="beast.evolution.tree.SimpleRandomTree" estimate="false" initial="@Tree.t:tree" taxa="@transeurasian"/>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">
            <distribution id="YuleModel.t:tree" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:tree" tree="@Tree.t:tree"/>
            <prior id="bcov_alpha_prior.s:transeurasian" name="distribution" x="@bcov_alpha.s:transeurasian">
                <Uniform id="Uniform.1" name="distr"/>
            </prior>
            <prior id="bcov_s_prior.s:transeurasian" name="distribution" x="@bcov_s.s:transeurasian">
                <Gamma id="Gamma.0" name="distr">
                    <parameter id="RealParameter.0" lower="0.0" name="alpha" upper="0.0">0.05</parameter>
                    <parameter id="RealParameter.1" lower="0.0" name="beta" upper="0.0">10.0</parameter>
                </Gamma>
            </prior>
            <prior id="YuleBirthRatePrior.t:tree" name="distribution" x="@birthRate.t:tree">
                <Uniform id="Uniform.2" name="distr"/>
            </prior>
            <prior id="MeanRatePrior.c:clock" name="distribution" x="@ucldMean.c:clock">
                <Uniform id="Uniform.4" name="distr" upper="Infinity"/>
            </prior>
            <prior id="ucldStdevPrior.c:clock" name="distribution" x="@ucldStdev.c:clock">
                <Gamma id="Gamma.1" name="distr">
                    <parameter id="RealParameter.4" estimate="false" name="alpha">0.5396</parameter>
                    <parameter id="RealParameter.5" estimate="false" name="beta">0.3819</parameter>
                </Gamma>
            </prior>
            <distribution id="Yayoi.prior" spec="beast.math.distributions.MRCAPrior" tree="@Tree.t:tree">
                <taxonset id="Yayoi" spec="TaxonSet">
                    <taxon id="Yuwan" spec="Taxon"/>
                    <taxon id="Yonaguni" spec="Taxon"/>
                    <taxon id="Shuri" spec="Taxon"/>
                    <taxon id="Okinoerabu" spec="Taxon"/>
                    <taxon id="Ogami" spec="Taxon"/>
                    <taxon id="Japanese" spec="Taxon"/>
                    <taxon id="Ikema" spec="Taxon"/>
                    <taxon id="Hateruma" spec="Taxon"/>
                    <taxon id="Tarama" spec="Taxon"/>
                </taxonset>
                <LogNormal id="LogNormalDistributionModel.0" meanInRealSpace="true" name="distr" offset="1700.0">
                    <parameter id="RealParameter.6" estimate="false" name="M">400.0</parameter>
                    <parameter id="RealParameter.7" estimate="false" lower="0.0" name="S" upper="5.0">0.6</parameter>
                </LogNormal>
            </distribution>
        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">
            <distribution id="treeLikelihood.transeurasian" spec="TreeLikelihood" tree="@Tree.t:tree" useAmbiguities="true">
                <data
id="orgdata.transeurasian"
spec="FilteredAlignment"
ascertained="true"
data="@transeurasian"
excludeto="1"
filter="-">
                    <userDataType id="TwoStateCovarion.0" spec="beast.evolution.datatype.TwoStateCovarion"/>
                </data>
                <siteModel id="SiteModel.s:transeurasian" spec="SiteModel" gammaCategoryCount="1" mutationRate="@mutationRate.s:transeurasian">
                    <parameter id="gammaShape.s:transeurasian" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:transeurasian" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="covarion" spec="BinaryCovarion" alpha="@bcov_alpha.s:transeurasian" switchRate="@bcov_s.s:transeurasian" vfrequencies="@frequencies.s:transeurasian">
                        <parameter id="hiddenfrequencies.s:transeurasian" dimension="2" estimate="false" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
                        <frequencies id="dummyfrequencies.s:transeurasian" spec="Frequencies" data="@transeurasian" estimate="false"/>
                    </substModel>
                </siteModel>
                <branchRateModel id="RelaxedClock.c:clock" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" clock.rate="@ucldMean.c:clock" rateCategories="@rateCategories.c:clock" tree="@Tree.t:tree">
                    <LogNormal id="LogNormalDistributionModel.c:clock" S="@ucldStdev.c:clock" meanInRealSpace="true" name="distr">
                        <parameter id="RealParameter.3" estimate="false" lower="0.0" name="M" upper="1.0">1.0</parameter>
                    </LogNormal>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>

    <operator id="bcovAlphaScaler.s:transeurasian" spec="ScaleOperator" parameter="@bcov_alpha.s:transeurasian" scaleFactor="0.75" weight="0.1"/>

    <operator id="bcovSwitchParamScaler.s:transeurasian" spec="ScaleOperator" parameter="@bcov_s.s:transeurasian" scaleFactor="0.75" weight="0.1"/>

    <operator id="YuleBirthRateScaler.t:tree" spec="ScaleOperator" parameter="@birthRate.t:tree" scaleFactor="0.75" weight="3.0"/>

    <operator id="frequenciesDelta.s:transeurasian" spec="DeltaExchangeOperator" delta="0.01" weight="0.1">
        <parameter idref="frequencies.s:transeurasian"/>
    </operator>

    <operator id="YuleModelTreeScaler.t:tree" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:tree" weight="3.0"/>

    <operator id="YuleModelTreeRootScaler.t:tree" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:tree" weight="3.0"/>

    <operator id="YuleModelUniformOperator.t:tree" spec="Uniform" tree="@Tree.t:tree" weight="30.0"/>

    <operator id="YuleModelSubtreeSlide.t:tree" spec="SubtreeSlide" tree="@Tree.t:tree" weight="15.0"/>

    <operator id="YuleModelNarrow.t:tree" spec="Exchange" tree="@Tree.t:tree" weight="15.0"/>

    <operator id="YuleModelWide.t:tree" spec="Exchange" isNarrow="false" tree="@Tree.t:tree" weight="3.0"/>

    <operator id="YuleModelWilsonBalding.t:tree" spec="WilsonBalding" tree="@Tree.t:tree" weight="3.0"/>

    <operator id="FixMeanMutationRatesOperator" spec="DeltaExchangeOperator" delta="0.75" weight="2.0">
        <parameter idref="mutationRate.s:transeurasian"/>
        <weightvector id="weightparameter" spec="parameter.IntegerParameter" estimate="false" lower="0" upper="0">227</weightvector>
    </operator>

    <operator id="ucldStdevScaler.c:clock" spec="ScaleOperator" parameter="@ucldStdev.c:clock" scaleFactor="0.5" weight="3.0"/>

    <operator id="CategoriesRandomWalk.c:clock" spec="IntRandomWalkOperator" parameter="@rateCategories.c:clock" weight="10.0" windowSize="1"/>

    <operator id="CategoriesSwapOperator.c:clock" spec="SwapOperator" intparameter="@rateCategories.c:clock" weight="10.0"/>

    <operator id="CategoriesUniform.c:clock" spec="UniformOperator" parameter="@rateCategories.c:clock" weight="10.0"/>

    <operator id="ucldMeanScaler.c:clock" spec="ScaleOperator" parameter="@ucldMean.c:clock" scaleFactor="0.5" weight="1.0"/>

    <operator id="relaxedUpDownOperator.c:clock" spec="UpDownOperator" scaleFactor="0.75" weight="3.0">
        <up idref="ucldMean.c:clock"/>
        <down idref="Tree.t:tree"/>
    </operator>

    <logger id="tracelog" fileName="OGTL_cov_rel_prior.log" logEvery="1000" model="@posterior" sort="smart">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log idref="treeLikelihood.transeurasian"/>
        <log id="TreeHeight.t:tree" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.t:tree"/>
        <log idref="mutationRate.s:transeurasian"/>
        <log idref="bcov_alpha.s:transeurasian"/>
        <log idref="bcov_s.s:transeurasian"/>
        <log idref="YuleModel.t:tree"/>
        <log idref="birthRate.t:tree"/>
        <log idref="frequencies.s:transeurasian"/>
        <log idref="ucldStdev.c:clock"/>
        <log id="rate.c:clock" spec="beast.evolution.branchratemodel.RateStatistic" branchratemodel="@RelaxedClock.c:clock" tree="@Tree.t:tree"/>
        <log idref="Yayoi.prior"/>
        <log idref="ucldMean.c:clock"/>
    </logger>

    <logger id="screenlog" logEvery="1000">
        <log idref="posterior"/>
        <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
    </logger>

    <logger id="treelog.t:tree" fileName="OGTL_cov_rel_prior.trees" logEvery="1000" mode="tree">
        <log id="TreeWithMetaDataLogger.t:tree" spec="beast.evolution.tree.TreeWithMetaDataLogger" branchratemodel="@RelaxedClock.c:clock" tree="@Tree.t:tree"/>
    </logger>

</run>

