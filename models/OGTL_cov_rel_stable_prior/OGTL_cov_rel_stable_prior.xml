<run id="mcmc" spec="MCMC" chainLength="10000000" sampleFromPrior="true">
    <state id="state" storeEvery="5000">
        <tree id="Tree.t:tree" name="stateNode">
            <taxonset id="TaxonSet.transeurasian_stable" spec="TaxonSet">
                <alignment idref="transeurasian_stable"/>
